<UserControl x:Class="MainMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FeneMonManagerWPF"
             xmlns:lib="clr-namespace:FeneMonLib;assembly=FeneMonLib"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Orientation="Vertical">
        <Button Command="{Binding NewGameCommand}">New Game</Button>
        <Button Command="{Binding LoadGameCommand}">Load Game</Button>
    </StackPanel>
</UserControl>
